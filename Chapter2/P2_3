import java.util.Scanner;

//Write a program that reads a number and displays the square, cube, and fourth
//power. Use the Math.pow method only for the fourth power.

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner in = new Scanner(System.in);
		System.out.print("Enter the number: ");
		double number = in.nextDouble();
		System.out.println("The square is: " + number*number);
		System.out.println("The cube is: " + number*number*number);
		System.out.println("The fourth power is " + Math.pow(number, 4));
	}

}
